{
  "env": {
    "development": {
      "presets": [
        "react",
        [
          "es2015",
          {
            "modules": false
          }
        ]
      ],
      "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties",
        "external-helpers"
      ]
    },
    "test": {
      "presets": [
        "react",
        "es2015"
      ],
      "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties"
      ]
    }
  }
}
